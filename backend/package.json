{
  "name": "agentic-ai-backend",
  "version": "1.0.0",
  "description": "Backend API for Agentic AI with advanced RAG architectures",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "node rag-examples.js all",
    "test:self-rag": "node rag-examples.js self-rag",
    "test:agentic-rag": "node rag-examples.js agentic-rag",
    "test:graph-rag": "node rag-examples.js graph-rag",
    "test:hyde-rag": "node rag-examples.js hyde-rag",
    "test:corrective-rag": "node rag-examples.js corrective-rag",
    "test:comparison": "node rag-examples.js comparison",
    "test:status": "node rag-examples.js status",
    "test:upload": "node rag-examples.js upload",
    "setup-data": "mkdir -p data/uploads && echo 'Data directories created'",
    "lint": "eslint *.js",
    "format": "prettier --write *.js"
  },
  "keywords": [
    "ai",
    "rag",
    "retrieval-augmented-generation",
    "machine-learning",
    "nlp",
    "api",
    "nodejs",
    "express"
  ],
  "author": "Agentic AI Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kulbirminhas-aiinitiative/agentic-ai.git"
  },
  "bugs": {
    "url": "https://github.com/kulbirminhas-aiinitiative/agentic-ai/issues"
  },
  "homepage": "https://github.com/kulbirminhas-aiinitiative/agentic-ai#readme"
}
